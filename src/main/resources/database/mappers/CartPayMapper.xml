<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.project.travel.cartPay.CartPayMapper">
	<insert id="setAdd" parameterType="CartPayVO">
		INSERT INTO TCARTPAY(PAYNUM,CARTNUM,NUM,PRODUCTNUM)
		VALUES(NULL,#{cartNum}, #{num},#{productNum})
	</insert>
</mapper>