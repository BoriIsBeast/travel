<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.travel.Tbest.TbestMapper">

<select id="list" parameterType="TbestVO" resultType="TbestVO">
	SELECT * FROM TBEST WHERE NUM = #{num}
</select>

<insert id="add" parameterType="TbestVO" useGeneratedKeys="true" keyProperty="num">
	INSERT INTO TBEST (NUM, PRODUCTNUM, ID, CATEGORY, TITLE, DETAIL)
	VALUES(#{num}, #{productNum}, #{id}, #{category}, #{title}, #{detail})
</insert>


</mapper>